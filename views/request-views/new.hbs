<h1>CREATE A NEW REQUEST</h1>

<form action="/requests/create" method="POST">
  <label for="qualification">Qualifications Required:</label><br>
  <input type="checkbox" name="qualification" value="Color Blind">Color Blind<br>
  <input type="checkbox" name="qualification" value="Low Vision">Low Vision<br>
  <input type="checkbox" name="qualification" value="Screen Reader">Screen Reader<br>
  <input type="checkbox" name="qualification" value="Hearing Impared">Hearing Impared<br>
  <input type="checkbox" name="qualification" value="Dyslexia">Dyslexia<br>
  <input type="checkbox" name="qualification" value="Physical Handicap">Physical Handicap<br>

  <label for="webURL">URL of Website/WebApp to be tested</label>
  <input type="text" name="webURL" id="webURL"><br>

  <button>Submit</button>
</form>